@model ProjectD.Models.SessionStartModel
    <!-- Leave alert function -->
@*<script>window.onbeforeunload = function () {
        return "Are you sure that you want to leave the session?";
    }</script>*@
<div class="container">
    <br />
    @ViewBag.SessionMessage
    <div class="row">
        <div class="col-sm-6" style="margin-top:10px">
            <div class="card">
                <div class="card-body">
                    <h5 class="card-title">Whiteboard</h5>
                    @*<p class="card-text">With supporting text below as a natural lead-in to additional content.</p>*@
                <a href="@Url.Action("Index", "Whiteboard")" class="btn btn-dark btn-lg">Go</a>
                </div>
            </div>
        </div>
        <div class="col-sm-6" style="margin-top:10px">
            <div class="card">
                <div class="card-body">
                    <h5 class="card-title">Vote</h5>
                    @*<p class="card-text">With supporting text below as a natural lead-in to additional content.</p>*@
                    <a href="@Url.Action("Index", "Poll")" class="btn btn-dark btn-lg">Go</a>
                </div>
            </div>
        </div>
        <br />
        <div class="col-sm-6" style="margin-top:10px">
            <div class="card">
                <div class="card-body">
                    <h5 class="card-title">Upload files</h5>
                    @*<p class="card-text">With supporting text below as a natural lead-in to additional content.</p>*@
                    <a href="#" class="btn btn-dark btn-lg">Go</a>
                </div>
            </div>
        </div>
        <div class="col-sm-6" style="margin-top:10px">
            <div class="card">
                <div class="card-body">
                    <h5 class="card-title">Leave Session</h5>
                    @*<p class="card-text">With supporting text below as a natural lead-in to additional content.</p>*@
                    <a href="@Url.Action("LeaveSession", "SessionStart")" class="btn btn-dark btn-lg">Go</a>
                </div>
            </div>
        </div>
    </div>
</div>

